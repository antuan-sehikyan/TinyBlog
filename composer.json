{
    "name": "ledsinclouds/tiny-blog",
    "description": "This is a ZF2 Tiny Blog Module using Doctrine2 + CkEditor + Htmlpurifier Integration with Soflomo Module. ",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "blog"
    ],
    "homepage": "https://github.com/antuan-sehikyan/TinyBlog.git",
    "minimum-stability": "dev",
    "repositories": [
		{
			"type": "vcs", 
			"url": "https://github.com/antuan-sehikyan/TinyBlog.git"
		}
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/doctrine-orm-module": "0.*"	
    },
    "autoload": {
        "psr-0": {
            "TinyBlog\\": "src/"
        }
    }
}  
